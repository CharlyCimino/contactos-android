<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="caemci.aplicacioncontactos">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="App Contactos INSPT"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="App Contactos INSPT">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityAgregar" android:label="Agregar contacto"/>
        <activity android:name=".ActivityModificar" android:label="Modificar contacto" />
        <activity android:name=".ActivityListar" android:label="Lista de contactos" />
        <activity android:name=".ActivityVer" android:label="Detalle" />
    </application>

</manifest>